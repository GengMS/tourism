ol>\n                <el-form-item\n                  prop=\"password\"\n                  :rules=\"[{ required: true, message: '密码不能为空' }]\"\n                >\n                  <div class=\"form-line\">\n                    <i class=\"el-icon-service input-icon\"></i>\n                    <el-input\n                      type=\"password\"\n                      placeholder=\"密码\"\n                      v-model=\"user.password\"\n                    ></el-input>\n                  </div>\n                </el-form-item>\n              </el-col>\n            </el-row>\n\n            <el-row class=\"form-item\">\n              <el-button\n                type=\"primary\"\n                class=\"submit-btn\"\n                size=\"small\"\n                @click=\"submitBtn\"\n              >\n                注册\n              </el-button>\n            </el-row>\n          </div>\n        </el-form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport BasicContainer from \"@vue-materials/basic-container\";\nconst backgroundImage = require(\"../../assets/CoUBYGBhNV-AN-SPADHuD9JYNGM647.png\");\nexport default {\n  components: { BasicContainer },\n  name: \"UserLogin\",\n\n  data() {\n    return {\n      backgroundImage: backgroundImage,\n      user: {\n        username: \"\",\n        password: \"\",\n      },\n      num:\"登录\",\n      disabled:false\n    };\n  },\n\n  created() {\n    if(this.$cookies.get(\"registy\") == null || this.$cookies.get(\"registy\") <= 0 ||this.$cookies.get(\"registy\") == 60) {\n      this.$cookies.set(\"registy\",60);\n      this.$cookies.set(\"registy\",60);\n      this.disabled = false;\n      this.num = \"获取\";\n    }else if(this.$cookies.get(\"registy\") < 60 && this.$cookies.get(\"registy\")> 0 ){\n      this.getCode();\n    }\n  },\n\n  methods: {\n    submitBtn() {\n      this.$refs[\"form\"].validate((valid) => {\n        if (valid) {\n          this.$message({\n            message: \"登录成功\",\n            type: \"success\",\n          });\n        }\n      });\n    },\n    getCode(){\n      let a = setInterval(() => {\n        if( this.$cookies.get(\"registy\") <= 0 ){\n          this.disabled = false;\n          this.num = \"获取\";\n          clearInterval(a);\n          \n        }else{\n            this.disabled = true;\n            this.$cookies.set(\"registy\",this.$cookies.get(\"registy\")-1);\n            this.num = this.$cookies.get(\"registy\");\n        }\n      \n       \n      }, 1000);\n    }\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"./UserLogin.scss\";\n.form-item-geng{\n  margin-bottom: 10px;\n}\n</style>\n"],"sourceRoot":"src/components/Registy"}]}